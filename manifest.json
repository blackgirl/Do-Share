{
  "name": "Night Watch",
  "description": "",
  "version": "0",
  "background_page": "background.html",
  "app": {
    "launch": {
      "local_path": "main.html"
    }
  },
  "icons": {
    "128": "icon128.png"
  },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "plusone.js" ],
      "matches": [ "https://plusone.google.com/*",
                   "https://plus.google.com/_/+1/*" ],
      "run_at": "document_start"
   } ],
   "permissions": [ "notifications",
                    "unlimitedStorage",
                    "https://plus.google.com/*",
                    "https://plusone.google.com/*",
                    "background"
   ]
}
