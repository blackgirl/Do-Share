{
  "name": "Do Share",
  "description": "Manage your posts on Google+â„¢",
  "version": "1.0.17",
  "background_page": "background.html",
  "options_page": "privacy.html",
  "icons": {
    "128": "icon128.png"
  },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "plusone.js" ],
      "matches": [ "https://plusone.google.com/*",
                   "https://plus.google.com/_/+1/*",
                   "https://plus.google.com/share*" ],
      "run_at": "document_start"
   },
   {
    "js": [ "photo_inject.js" ],
    "matches": [ "http://plus.google.com/*", "https://plus.google.com/*" ],
    "run_at": "document_end"
   },
   {
    "all_frames": true,
    "js": [ "stream_inject.js" ],
    "matches": [ "http://plus.google.com/*", "https://plus.google.com/*" ],
    "run_at": "document_start"
   } ],
   "browser_action": {
     "default_title": "Do Share"
   },
   "permissions": [ "notifications",
                    "unlimitedStorage",
                    "https://plus.google.com/*",
                    "https://plusone.google.com/*",
                    "background"
   ],
   "minimum_chrome_version": "18"
}
